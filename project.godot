; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

[application]

config/name="Godot Translation"
run/main_scene="res://main.tscn"
config/icon="res://icon.png"

[display]

window/size/width=720
window/size/height=380

[locale]

translations=PoolStringArray( "res://text.es_ES.translation", "res://text.ja_JP.translation", "res://text.lo_LA.translation", "res://text.th_TH.translation", "res://text.en_GB.translation" )
translation_remaps={
"res://assets/flag-united-kingdom.png": PoolStringArray( "res://assets/flag-spain.png:es", "res://assets/flag-japan.png:ja", "res://assets/flag-laos.png:lo_LA", "res://assets/flag-thailand.png:th" )
}

[physics]

common/enable_pause_aware_picking=true

[rendering]

quality/driver/driver_name="GLES2"
vram_compression/import_etc=true
vram_compression/import_etc2=false
environment/default_environment="res://default_env.tres"
